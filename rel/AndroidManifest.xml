    package="com.microsoft.rdc.androidx"
    xmlns:tools="http://schemas.android.com/tools">
    <application>
        <!-- This MSAL activity is put in the debug specific manifest because we have different
             base64 encoded package signature between the debug apk vs the release apk
        -->
