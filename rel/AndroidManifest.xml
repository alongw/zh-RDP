            tools:node="merge">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:host="com.microsoft.rdc.androidx"
                    android:path="/AAXfoSjc4WJkquYRQfLtK2iKoU0="
                    android:scheme="msauth" />
                <!-- AAXfoSjc4WJkquYRQfLtK2iKoU0%3d -->
            </intent-filter>
        </activity>
    </application>
</manifest>